fn fibonacci(limit: int32): int32 {
    let mut i: int32 = 0; 
    let mut out: int32 = 0;
    let mut a1: int32 = 0;
    let mut a2: int32 = 1;
    while (i < limit) {
        out = a1 + a2;
        a1 = a2;
        a2 = out;
    }
    return out;
}

fn main(): int8 {
    let limit: int32 = 5;
    print(fibonacci(limit))
    return 0;
}